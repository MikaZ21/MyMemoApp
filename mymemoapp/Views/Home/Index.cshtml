@model mymemoapp.Models.ViewModels.MemoViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <div>
        <table>
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var m in Model.MemoList)
                {
                    <tr>
                        <td class="todos-column" scope="row">@m.Id</td>
                        <td class="todos-column">@m.Name</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
